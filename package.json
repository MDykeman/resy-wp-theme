{
  "name": "resy-wp-theme",
  "version": "1.0.0",
  "description": "Resume Single Page WordPress Theme",
  "main": "index.js",
  "scripts": {
    "start": "run-script-os",
    "start:win32": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser \"C:\\Program Files\\Firefox Developer Edition\\firefox.exe\"",
    "//": "Hello! If you are having trouble running this command. Try changing Firefox Developer Edition to FirefoxDeveloperEdition",
    "start:darwin:linux": "browser-sync start --server --files '**/*.css, **/*.html, **/*.js, !node_modules/**/*' --directory --port 7777 --browser 'Firefox Developer Edition'"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/MDykeman/resy-wp-theme.git"
  },
  "keywords": [
    "Wordpress",
    "One",
    "Page",
    "Resume",
    "Single",
    "Page",
    "Responsive"
  ],
  "author": "Matthew Dykeman",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/MDykeman/resy-wp-theme/issues"
  },
  "homepage": "https://github.com/MDykeman/resy-wp-theme#readme",
  "devDependencies": {
    "browser-sync": "^2.24.7",
    "run-script-os": "^1.0.3"
  }
}